{
  "label": "District labels",
  "source": {
    "type": "geojson",
    "data": "data/ipc_distrtics_centroids.geojson"
  },
  "type": "symbol",
  "minzoom": 5.5,
  "paint": {
    "text-color": "#000",
    "text-halo-color": "#fff",
    "text-halo-width": 1.3,
    "text-halo-blur": 1
  },
  "layout": {
    "text-size": 12,
    "text-field": "{name}",
    "text-offset": [
      0,
      3
    ]
  },
  "visible": false
}
